  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\GraphQL.Api.Server\GraphQL.Api.Server.csproj" />
    <ProjectReference Include="..\GraphQL.Api\GraphQL.Api.csproj" />
  </ItemGroup>

  <ItemGroup>
    <InternalsVisibleTo Include="Mt.GraphQL.Api.Test" />
  </ItemGroup>
  
</Project>
